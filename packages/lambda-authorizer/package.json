{
  "name": "verified-permissions-lambda-authorizer",
  "version": "0.0.1",
  "private": false,
  "description": "AWS Lambda Authorizer for AWS Verified Permissions (TypeScript, bundled)",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint src --max-warnings 0",
    "format": "prettier --check .",
    "format:write": "prettier --write ."
  },
  "dependencies": {
    "@aws-sdk/client-verifiedpermissions": "^3.671.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.146",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-promise": "^7.1.0",
    "prettier": "^3.3.3",
    "tslib": "^2.8.1",
    "tsup": "^8.2.4",
    "typescript": "^5.6.3"
  }
}
